<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.warehouse.mapper.StockMapper">

    <select id="getStockByPosition" parameterType="java.lang.String" resultType="com.example.warehouse.entity.Stock">
        SELECT *
        FROM stock
        WHERE position = #{position}
    </select>

</mapper>